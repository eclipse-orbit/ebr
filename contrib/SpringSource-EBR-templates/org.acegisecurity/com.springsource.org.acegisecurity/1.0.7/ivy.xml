<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://ivyrep.jayasoft.org/ivy-doc.xsl"?>
<ivy-module
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd"
		version="1.3">

	<info organisation="org.acegisecurity" module="com.springsource.org.acegisecurity"/>

	<configurations>
		<include file="${spring.build.dir}/common/default-ivy-configurations.xml"/>
	</configurations>

	<publications>
		<artifact name="${ant.project.name}"/>
		<artifact name="${ant.project.name}-sources" type="src" ext="jar"/>
		<artifact name="license" type="license" ext="txt"/>
		<artifact name="notice" type="license" ext="txt"/>
	</publications>

	<dependencies>
		<dependency org="edu.yale.its.tp.cas" name="com.springsource.edu.yale.its.tp.cas" rev="2.0.11" conf="optional->runtime"/>
		<dependency org="javax.servlet" name="com.springsource.javax.servlet" rev="2.4.0" conf="provided->runtime"/>
		<dependency org="javax.servlet" name="com.springsource.javax.servlet.jsp" rev="2.0.0" conf="provided->runtime"/>
		<dependency org="net.sourceforge.ehcache" name="com.springsource.net.sf.ehcache" rev="1.2.4" conf="optional->runtime"/>
		<dependency org="org.aopalliance" name="com.springsource.org.aopalliance" rev="1.0.0" conf="compile->runtime"/>
		<dependency org="org.apache.commons" name="com.springsource.org.apache.commons.codec" rev="1.3.0" conf="compile->runtime"/>
		<dependency org="org.apache.commons" name="com.springsource.org.apache.commons.collections" rev="3.2.1" conf="compile->runtime"/>
		<dependency org="org.apache.commons" name="com.springsource.org.apache.commons.logging" rev="1.0.4" conf="compile->runtime"/>
		<dependency org="org.apache.oro" name="com.springsource.org.apache.oro" rev="2.0.8" conf="compile->runtime"/>
		<dependency org="org.aspectj" name="com.springsource.org.aspectj.runtime" rev="1.6.3.RELEASE" conf="optional->runtime"/>
		<dependency org="org.springframework" name="org.springframework.aop" rev="2.5.6.SEC01" conf="compile->runtime"/>
		<dependency org="org.springframework" name="org.springframework.beans" rev="2.5.6.SEC01" conf="compile->runtime"/>
		<dependency org="org.springframework" name="org.springframework.context" rev="2.5.6.SEC01" conf="compile->runtime"/>
		<dependency org="org.springframework" name="org.springframework.core" rev="2.5.6.SEC01" conf="compile->runtime"/>
		<dependency org="org.springframework" name="org.springframework.jdbc" rev="2.5.6.SEC01" conf="compile->runtime"/>
		<dependency org="org.springframework" name="org.springframework.test" rev="2.5.6.SEC01" conf="optional->runtime"/>
		<dependency org="org.springframework" name="org.springframework.transaction" rev="2.5.6.SEC01" conf="compile->runtime"/>
		<dependency org="org.springframework" name="org.springframework.web" rev="2.5.6.SEC01" conf="optional->runtime"/>
	</dependencies>

</ivy-module>
