<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://ivyrep.jayasoft.org/ivy-doc.xsl"?>
<ivy-module
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd"
		version="1.3">

	<info organisation="net.sourceforge.jasperreports" module="com.springsource.net.sf.jasperreports"/>

	<configurations>
		<include file="${spring.build.dir}/common/default-ivy-configurations.xml"/>
	</configurations>

	<publications>
		<artifact name="${ant.project.name}"/>
		<artifact name="${ant.project.name}-sources" type="src" ext="jar"/>
		<artifact name="license" type="license" ext="txt"/>
	</publications>

	<dependencies>
		<dependency org="org.antlr" name="com.springsource.antlr" rev="2.7.7" conf="optional->compile"/>
		<dependency org="org.beanshell" name="com.springsource.bsh" rev="2.0.0.b4" conf="optional->compile"/>
		<dependency org="com.lowagie.text" name="com.springsource.com.lowagie.text" rev="2.0.8" conf="compile->compile"/>
		<dependency org="javax.persistence" name="com.springsource.javax.persistence" rev="1.0.0" conf="provided->compile"/>
		<dependency org="javax.servlet" name="com.springsource.javax.servlet" rev="2.5.0" conf="provided->compile"/>
		<dependency org="net.sourceforge.jexcelapi" name="com.springsource.jxl" rev="2.6.6" conf="optional->compile"/>
		<dependency org="org.apache.batik" name="com.springsource.org.apache.batik.bridge" rev="1.7.0" conf="optional->compile"/>
		<dependency org="org.apache.batik" name="com.springsource.org.apache.batik.dom" rev="1.7.0" conf="optional->compile"/>
		<dependency org="org.apache.batik" name="com.springsource.org.apache.batik.dom.svg" rev="1.7.0" conf="optional->compile"/>
		<dependency org="org.apache.batik" name="com.springsource.org.apache.batik.ext.awt" rev="1.7.0" conf="optional->compile"/>
		<dependency org="org.apache.batik" name="com.springsource.org.apache.batik.gvt" rev="1.7.0" conf="optional->compile"/>
		<dependency org="org.apache.batik" name="com.springsource.org.apache.batik.svggen" rev="1.7.0" conf="optional->compile"/>
		<dependency org="org.apache.commons" name="com.springsource.org.apache.commons.beanutils" rev="1.7.0" conf="compile->compile"/>
		<dependency org="org.apache.commons" name="com.springsource.org.apache.commons.collections" rev="3.2.0" conf="compile->compile"/>
		<dependency org="org.apache.commons" name="com.springsource.org.apache.commons.digester" rev="1.8.0" conf="compile->compile"/>
		<dependency org="org.apache.commons" name="com.springsource.org.apache.commons.logging" rev="1.1.1" conf="compile->compile"/>
		<dependency org="org.apache.poi" name="com.springsource.org.apache.poi" rev="3.0.2.FINAL" conf="optional->compile"/>
		<dependency org="org.apache.ant" name="com.springsource.org.apache.tools.ant" rev="1.7.0" conf="optional->compile"/>
		<dependency org="org.apache.xalan" name="com.springsource.org.apache.xalan" rev="2.7.0" conf="optional->compile"/>
		<dependency org="org.codehaus.groovy" name="com.springsource.org.codehaus.groovy" rev="1.5.1" conf="optional->compile"/>
		<dependency org="org.eclipse.jdt" name="com.springsource.org.eclipse.jdt.core.compiler.batch" rev="3.3.0" conf="compile->compile"/>
		<dependency org="org.hibernate" name="com.springsource.org.hibernate" rev="3.2.6.ga" conf="optional->compile"/>
		<dependency org="org.jaxen" name="com.springsource.org.jaxen" rev="1.1.1" conf="optional->compile"/>
		<dependency org="org.jfree" name="com.springsource.org.jfree" rev="1.0.12" conf="optional->compile"/>
		<dependency org="org.jfree.chart" name="com.springsource.org.jfree.chart" rev="1.0.9" conf="optional->compile"/>
	</dependencies>

</ivy-module>
