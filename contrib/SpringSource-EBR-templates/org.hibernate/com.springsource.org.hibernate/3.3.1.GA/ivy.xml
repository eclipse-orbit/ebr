<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://ivyrep.jayasoft.org/ivy-doc.xsl"?>
<ivy-module
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd"
		version="1.3">

	<info organisation="org.hibernate" module="com.springsource.org.hibernate"/>

	<configurations>
		<include file="${spring.build.dir}/common/default-ivy-configurations.xml"/>
	</configurations>

	<publications>
		<artifact name="${ant.project.name}"/>
		<artifact name="${ant.project.name}-sources" type="src" ext="jar"/>
		<artifact name="license" type="license" ext="txt"/>
	</publications>

	<dependencies>
		<dependency org="javax.transaction" name="com.springsource.javax.transaction" rev="1.1.0" conf="provided->compile"/>
		<dependency org="org.apache.ant" name="com.springsource.org.apache.tools.ant" rev="1.7.0" conf="optional->compile"/>
		<dependency org="org.apache.commons" name="com.springsource.org.apache.commons.collections" rev="3.2.0" conf="compile->compile"/>
		<dependency org="org.antlr" name="com.springsource.antlr" rev="2.7.6" conf="compile->compile"/>
		<dependency org="com.mchange.c3p0" name="com.springsource.com.mchange.v2.c3p0" rev="0.9.1" conf="optional->compile"/>
		<dependency org="com.opensymphony.oscache" name="com.springsource.com.opensymphony.oscache" rev="2.1.0" conf="optional->compile"/>
		<dependency org="org.jboss.javassist" name="com.springsource.javassist" rev="3.3.0.ga" conf="compile->compile"/>
		<dependency org="org.jgroups" name="com.springsource.org.jgroups" rev="2.2.8" conf="compile->compile"/>
		<dependency org="net.sourceforge.cglib" name="com.springsource.net.sf.cglib" rev="2.1.3" conf="compile->compile"/>
		<dependency org="net.sourceforge.ehcache" name="com.springsource.net.sf.ehcache" rev="1.2.3" conf="optional->compile"/>
		<dependency org="net.sourceforge.swarmcache" name="com.springsource.net.sf.swarmcache" rev="1.0.0.RC2a" conf="optional->compile"/>
		<dependency org="org.dom4j" name="com.springsource.org.dom4j" rev="1.6.1" conf="compile->compile"/>
		<dependency org="org.logicalcobwebs" name="com.springsource.org.logicalcobwebs.proxool" rev="0.8.3" conf="optional->compile"/>
		<dependency org="org.objectweb.asm" name="com.springsource.org.objectweb.asm" rev="1.5.3" conf="compile->compile"/>
		<dependency org="org.objectweb.asm" name="com.springsource.org.objectweb.asm.attrs" rev="1.5.3" conf="compile->compile"/>
		<dependency org="org.slf4j" name="com.springsource.slf4j.api" rev="1.5.3" conf="compile->compile"/>
	</dependencies>

</ivy-module>
