<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://ivyrep.jayasoft.org/ivy-doc.xsl"?>
<ivy-module
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd"
		version="1.3">

	<info organisation="com.sun.xml" module="com.springsource.com.sun.xml.bind.jaxb1"/>

	<configurations>
		<include file="${spring.build.dir}/common/default-ivy-configurations.xml"/>
	</configurations>

	<publications>
		<artifact name="${ant.project.name}"/>
		<artifact name="license" type="license" ext="txt"/>
	</publications>
	
	<dependencies>
		<dependency org="com.sun.msv" name="com.springsource.com.sun.msv.datatype" rev="0.0.0.20041122" conf="compile->compile"/>
		<dependency org="com.sun.xml" name="com.springsource.com.sun.xml.bind" rev="2.1.7" conf="compile->compile"/>
		<dependency org="org.apache.xerces" name="com.springsource.org.apache.xerces" rev="2.8.1" conf="compile->compile"/>
		<dependency org="javax.xml.bind" name="com.springsource.javax.xml.bind" rev="2.1.7" conf="provided->compile"/>
		<dependency org="org.apache.xml" name="com.springsource.org.apache.xml.resolver" rev="1.2.0" conf="compile->compile"/>
	</dependencies>

</ivy-module>
